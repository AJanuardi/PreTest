<h1>Register</h1>

<label>User</label>
<form method="POST" enctype="multipart/form-data" asp-action="AddedUser" asp-controller="Home">
<input id="name" name="name" placeholder="name">
<input id="birthdate" name="birthdate" placeholder="birthdate">
<input id="phone" name="phone" placeholder="phone">
<input id="address" name="address" placeholder="address">
<input type="file" id="cv" name="cv" placeholder="cv">
<input type="file" id="photo" name="photo" placeholder="photo">
<input id="username" name="username" placeholder="username">
<input type="password" name="password" id="password" placeholder="password">
<input id="email" name="email" placeholder="email">
<input type="submit">
</form>
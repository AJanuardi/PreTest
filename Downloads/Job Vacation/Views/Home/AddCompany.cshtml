<h1>Register</h1>

<div class="container">
    <div class="row">
        <div class="col-md-2"></div>
        <div class="col-md-8" style="text-align: center;">
            <form method="POST" asp-action="AddedCompany" asp-controller="Home" enctype="multipart/form-data">
            <div class="form-group">
            <input class="form-control" id="name" name="name" placeholder="name">
            </div>
            <div class="form-group">
            <input class="form-control" id="phone" name="phone" placeholder="phone">
            </div>
            <div class="form-group">
            <input class="form-control" id="address" name="address" placeholder="address">
            </div>
            <div class="form-group">
            <input type="file" name="logo" id="logo" placeholder="logo">
            </div>
            <div class="form-group">
            <input class="form-control" id="username" name="username" placeholder="username">
            </div>
            <div class="form-group">
            <input class="form-control" type="password" name="password" id="password" placeholder="password">
            </div>
            <div class="form-group">
            <input class="form-control" id="email" name="email" placeholder="email">
            </div>
            <input type="submit" class="btn btn-success">
            </form> 
        </div>
    </div>
</div>